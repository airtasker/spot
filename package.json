    "@types/qs": "^6.5.3",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^10.11.3",
    "ajv": "^6.10.2",
    "assert-never": "^1.2.0",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.14",
    "qs": "^6.9.0",
    "randomstring": "^1.1.5",
    "supertest": "^4.0.2",
    "ts-morph": "^4.3.3",
