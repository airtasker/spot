  },
  "bugs": "https://github.com/airtasker/spot/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "ajv": "^6.12.6",
    "assert-never": "^1.2.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "inquirer": "^7.3.3",
    "js-yaml": "^4.0.0",
    "qs": "^6.9.6",
    "randomstring": "^1.1.5",
