    "@types/jest": "^24.0.21",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.145",
    "@types/moxios": "^0.4.8",
    "@types/node": "^12.12.6",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "core-js": "^3.3.6",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mobx": "^5.14.2",
    "nock": "^11.7.0",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "redoc": "^2.0.0-rc.18",
    "speccy": "^0.11.0",
    "styled-components": "^4.4.1",
